# Ollama Configuration
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=llama3

# API Parameters
TEMPERATURE=0.7
TOP_P=0.9
MAX_TOKENS=2048

# Data Generation Settings
EXAMPLES_PER_BATCH=5
OUTPUT_DIR=./data

# Ethical Categories
# Comma-separated list of ethical categories to include in generation
ETHICAL_CATEGORIES=privacy,fairness,autonomy,harm,deception,general_ethics

# Logging
LOG_LEVEL=INFO